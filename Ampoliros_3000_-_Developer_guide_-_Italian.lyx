\layout LyX-Code

MODULE_AUTHOR_EMAIL = info@solarix.biz
\layout LyX-Code

MODULE_AUTHOR_SITE = http://www.solarix.biz/
\layout LyX-Code

MODULE_SUPPORT_EMAIL = support@ampoliros.com
\layout LyX-Code

MODULE_BUGS_EMAIL = bugs@ampoliros.com
\layout LyX-Code

MODULE_COPYRIGHT = Copyright (c) 2000-2002 Solarix
\layout LyX-Code

MODULE_LICENSE = GPL
\layout LyX-Code

MODULE_LICENSE_FILE = LICENSE
\layout LyX-Code

